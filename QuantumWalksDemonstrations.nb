            RowBox[{"func", "@", "#"}]}], ")"}], "&"}], ",", "target"}], 
        "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"progressBar", "[", 
        RowBox[{"100.", 
         RowBox[{"iter", "/", 
          RowBox[{"Length", "@", "target"}]}]}], "]"}]}], 
