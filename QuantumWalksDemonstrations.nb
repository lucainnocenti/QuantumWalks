 RowBox[{
  RowBox[{
   RowBox[{"monitorMap", "[", 
    RowBox[{"Map", "[", 
     RowBox[{"func_", ",", "target_"}], "]"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
